{
	"compilerOptions": {
		"composite": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"emitDeclarationOnly": true,
		"allowImportingTsExtensions": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["functions"]
}
